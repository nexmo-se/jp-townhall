{
  "name": "ot-townhall",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "scripts": {
    "start:frontend:prod": "cd frontend && yarn start:prod",
    "start:backend:prod": "cd backend && yarn start:prod",
    "start:frontend:dev": "cd frontend && yarn start",
    "start:backend:dev": "cd backend && yarn start:dev",
    "start:prod": "concurrently \"yarn start:frontend:prod\" \"yarn start:backend:prod\"",
    "start:dev": "concurrently \"yarn start:backend:dev\" \"yarn start:frontend:dev\""
  }
}
